+
+int
+sys_setPrio(void){
+  int n;
+
+  if(argint(0,&n) < 0 || n <= 0 || n > 1000){
+    return -1;
+  }
+
+  myproc()->priority = n;
+  return 0;
+}
+
+int
+sys_getPrio(void){
+  return myproc()->priority;
