index 0000000..88620a1
--- /dev/null
+++ b/testPrio.c
@@ -0,0 +1,42 @@
+#include "types.h"
+#include "stat.h"
+#include "user.h"
+
+void createProc(int priority)
+{
+    int pid = fork();
+    if (pid < 0)
+    {
+        printf(1, "Fork failed\n");
+        exit();
+    }
+    else if (pid == 0)
+    {
+        setPrio(priority);
+        for(int i = 0 ; i < 100;i++){
+            sleep(10);
+            printf(1,"proces o priorytecie: %d iteracja petli: %d \n",getPrio(),i);
+        }
+        printf(1, "proces o priorytecie = %d zakonczyl sie\n", getPrio());
+        exit();
+    }
+}
+
+int main(int argc, char *argv[])
+{
+    printf(1,"Start testu, oczekiwanie na procesy\n");
+    createProc(100);
+    createProc(300);
+    createProc(500);
+    createProc(150);
+    createProc(200);
+
+    
+
+    for (int i = 0; i < 8; i++)
+    {
+        wait();
+    }
+
+    exit();
+}
